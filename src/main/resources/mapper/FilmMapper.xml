<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hand.mapper.FilmMapper">
    <insert id="insert" parameterType="com.hand.dto.Film" useGeneratedKeys="true" keyProperty="film_id">
        insert into film(title, description, language_id)
        VALUES (#{title}, #{description}, #{languageId})
    </insert>
</mapper>